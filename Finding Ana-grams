```javascript
//declare two functions , 
//convert them to lowercase, compare the letters they have
function ang(str1, str2){
let strNew = str1.toLowerCase().replace(/[a-z]/g, ' ')
let strNew2 = str2.toLowerCase().replace(/[a-z]/g, ' ')
return strNew === strNew2
}
let stri = "note";
let str3 = "toen"
console.log(ang(stri,str3)) 
```
